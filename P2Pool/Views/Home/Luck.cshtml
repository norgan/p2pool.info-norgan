@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
	@Html.Partial("Tracking")
    <meta charset="utf-8">
    <title>Pool Luck - p2pool.info</title>
    <!-- Le HTML5 shim, for IE6-8 support of HTML elements -->
    <!--[if lt IE 9]>
      <script src="/scripts/html5.js"></script>
    <![endif]-->
    <!-- Le styles -->
    <link href="@Url.StyleSheet("bootstrap.min.css")" rel="stylesheet" />
    <link href="@Url.StyleSheet("bootstrap-responsive.css")" rel="stylesheet" />
    <link href="@Url.StyleSheet("p2pool.css")" rel="stylesheet" />
</head>
<body>
    <div class="navbar navbar-fixed-top">
        <div class="navbar-inner">
            <div class="container">
                <a href="/" class="homelink">
                    <img class="pull-left" src="@Url.Content("~/content/logo.png")" alt="p2pool" /></a>
                <ul class="nav">
                    <li><a href="/">Return to Stats</a></li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container" id="main-container">
        <div class="alert alert-error" style="display: none" data-bind="visible: !canShowChart && !dismissSvgAlert">
            <a class="close" data-dismiss="alert" id="alert-svg">×</a> The p2pool hash rate
            chart requires a browser that supports either SVG or VML. Your browser does not.
        </div>
        <div id="chart-luckovertime" class="chart">
            <div class="chart-loading">
                Loading...</div>
        </div>
        <div id="chart-blocksvshashes" class="chart">
            <div class="chart-loading">
                Loading...</div>
        </div>
        <script type="text/javascript" src="@Url.JavaScript("jquery-1.7.1.min.js")"></script>
        <script type="text/javascript" src="@Url.JavaScript("store-json2.min.js")"></script>
        <script type="text/javascript" src="@Url.JavaScript("knockout.js")"></script>
        <script type="text/javascript" src="@Url.JavaScript("knockout.mapping-latest.js")"></script>
        <script type="text/javascript" src="@Url.JavaScript("highstock.src.js")"></script>
        <script type="text/javascript" src="@Url.JavaScript("bootstrap.min.js")"></script>
        <script type="text/javascript" src="@Url.JavaScript("p2pool-luck.js")"></script>
    </div>
</body>
</html>
